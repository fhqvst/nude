{
  "name": "nude-css",
  "version": "0.0.3",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "styles": "echo '=> styles' && node-sass -q scss/nude.scss -o .",
    "styles:watch": "nodemon -q -e scss -w scss -x 'npm run styles'",
    "watch": "http-server & livereload & npm run styles:watch",
    "start": "npm run watch"
  },
  "devDependencies": {
    "http-server": "^0.9.0",
    "livereload": "^0.4.1",
    "node-sass": "^3.10.1",
    "nodemon": "^1.11.0"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "description": "A CSS framework built with speed and ease-of-use in mind",
  "main": "nude.css",
  "author": "Filip Hallqvist <filiphallqvist@gmail.com>",
  "license": "MIT"
}
